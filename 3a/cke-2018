#include<bits/stdc++.h>
using namespace std;

string T[1002];
void zad1();
void zad2();

int main()
{
	ifstream odczyt ("sygnaly.txt");
	for (int i=0 ; i < 1000 ; i++)
		odczyt>>T[i];
	odczyt.close();
	/*	
	for (int i=0 ; i < 1000 ; i++)
		cout<<T[i]<<endl;	
	*/
	zad1();
	zad2();
}

void zad1()
{
	string wynik="";
	char temp;
	for (int i=39 ; i < 1000 ; i+=40)
		{
			temp=T[i][9];
			wynik=wynik+temp;
		}
	cout<<wynik;
	ofstream zapisz ("wynik4.txt");
	zapisz<<"Odp1:\n";
	zapisz<<wynik;		
}

void zad2()
{
	set <char> S;
	string wynik="";
	int wynikL=0;
	int n;
	for (int i=0 ; i<1000 ; i++)
		{
			n=T[i].length();
			for (int j=0 ; j<n ; j++)
				S.insert(T[i][j]);
			if ( S.size() > wynikL )	
				{
					wynik=T[i];
					wynikL=S.size();					
				}
			S.clear();	
		}
	
	cout << "\nODP2\n" << wynik << " " << wynikL ;	
	ofstream out ("wynik4.txt",fstream::app);
	out << "\nODP2\n" << wynik << " " << wynikL ;	
	out.close();	
	
	/* sprawdzenie set*/
	/*
	S.insert('a');
	S.insert('a');
	S.insert('b');
	cout<< "\nSET SIZE=" << S.size();
	S.clear();
	cout<< "\nSET SIZE=" << S.size();
	*/
}
